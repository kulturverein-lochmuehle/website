    "@stencil/router": "2.0.0-2",
    "@stencil/sass": "3.2.3",
    "@types/ical": "0.8.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.1",
    "@types/node-fetch": "2.6.13",
    "autoprefixer": "10.4.22",
    "cross-env": "10.1.0",
