{
  "name": "@kvlm/ui",
  "description": "UI components for kvlm.de",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "David Enke",
    "email": "david@kvlm.de"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "customElements": "src/custom-elements.json",
  "files": [
    "astro",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*.js": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./global.css": "./dist/index.css"
  },
  "scripts": {
    "lint": "eslint . -c .eslintrc.json",
    "docs": "cross-env NODE_OPTIONS='--loader ts-node/esm --no-warnings' cem analyze --config cem.config.ts",
    "test:run": "cross-env NODE_OPTIONS='--loader ts-node/esm --no-warnings' wtr --config web-test-runner.config.ts",
    "test": "run-s -lns build:components test:run",
    "test:watch": "run-p -lns \"build:components --watch\" \"test:run --watch\"",
    "build:clean": "shx rm -rf dist",
    "build:index": "barrelsby --config .barrelsby.json",
    "build:components": "cross-env NODE_OPTIONS='--loader ts-node/esm --no-warnings' node esbuild.config.ts",
    "build:docs": "npm run docs",
    "build": "run-s -lns build:*",
    "dev:components": "npm run build:components -- --watch --port 3500",
    "dev:docs": "npm run docs -- --watch",
    "dev": "npm-run-all build:index build:docs --parallel --silent -ln dev:*"
  },
  "dependencies": {
    "lit": "3.1.1",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@csstools/css-parser-algorithms": "2.5.0",
    "@csstools/css-tokenizer": "2.2.3",
    "@custom-elements-manifest/analyzer": "0.9.0",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@lit-labs/testing": "0.2.3",
    "@open-wc/semantic-dom-diff": "0.20.1",
    "@open-wc/testing": "4.0.0",
    "@open-wc/testing-helpers": "3.0.0",
    "@types/browser-sync": "2.29.0",
    "@types/esbuild-copy-static-files": "0.1.3",
    "@types/lodash-es": "4.17.12",
    "@types/mocha": "10.0.6",
    "@types/node": "20.11.10",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "@web/dev-server-esbuild": "1.0.1",
    "@web/test-runner": "0.18.0",
    "@web/test-runner-commands": "0.9.0",
    "@web/test-runner-playwright": "0.11.0",
    "@webcomponents/template-shadowroot": "0.2.1",
    "@webcomponents-preview/cem-plugin-examples": "0.0.40",
    "@webcomponents-preview/cem-plugin-generate-readmes": "0.0.40",
    "@webcomponents-preview/cem-plugin-grouping": "0.0.40",
    "@webcomponents-preview/client": "0.7.4",
    "autoprefixer": "10.4.17",
    "axe-core": "4.8.3",
    "barrelsby": "2.8.1",
    "chai-a11y-axe": "1.5.0",
    "cross-env": "7.0.3",
    "esbuild": "0.20.0",
    "esbuild-copy-static-files": "0.1.0",
    "esbuild-sass-plugin": "2.16.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-file-extension-in-import-ts": "2.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-lit": "1.11.0",
    "eslint-plugin-lit-a11y": "4.1.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-unused-imports": "3.0.0",
    "eslint-plugin-require-extensions": "0.1.3",
    "fast-glob": "3.3.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.33",
    "postcss-preset-env": "9.3.0",
    "prettier": "3.2.4",
    "resolve": "1.22.8",
    "sass": "1.70.0",
    "shx": "0.3.4",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.8",
    "tslib": "2.6.2",
    "typedoc": "0.25.7",
    "typescript": "5.3.3"
  }
}
