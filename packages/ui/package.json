{
  "name": "@kvlm/ui",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "David Enke",
    "email": "david@enke.dev"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./global.css": "./dist/index.css",
    "./*": "./dist/*.js",
    "./*.js": "./dist/*.js"
  },
  "scripts": {
    "lint": "eslint --ext .ts src",
    "clean": "shx rm -rf dist",
    "generate": "barrelsby --config .barrelsby.json",
    "docs": "cross-env NODE_OPTIONS='--loader ts-node/esm' NODE_NO_WARNINGS=1 cem analyze --config cem.config.ts",
    "test:run": "cross-env NODE_OPTIONS='--loader ts-node/esm' NODE_NO_WARNINGS=1 wtr --config web-test-runner.config.ts",
    "test": "run-s -lns build:components test:run",
    "test:watch": "run-p -lns \"build:components --watch\" \"test:run --watch\"",
    "prebuild": "run-s -lns clean generate",
    "build:components": "cross-env NODE_OPTIONS='--loader ts-node/esm' NODE_NO_WARNINGS=1 node esbuild.config.ts",
    "build:types": "tsc --project tsconfig.types.json",
    "build:docs": "npm run docs",
    "build": "run-p -lns build:*",
    "predev": "run-s -lns clean generate",
    "dev:docs": "npm run docs -- --watch",
    "dev:components": "npm run build:components -- --watch --port 3500",
    "dev": "run-p -lns dev:*"
  },
  "dependencies": {
    "lit": "2.7.4",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@csstools/css-parser-algorithms": "2.1.1",
    "@csstools/css-tokenizer": "2.1.1",
    "@custom-elements-manifest/analyzer": "0.8.3",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@lit-labs/testing": "0.2.1",
    "@open-wc/semantic-dom-diff": "0.19.9",
    "@open-wc/testing": "3.1.8",
    "@open-wc/testing-helpers": "2.2.1",
    "@types/browser-sync": "2.27.0",
    "@types/lodash-es": "4.17.7",
    "@types/mocha": "10.0.1",
    "@types/node": "18.16.12",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "@web/dev-server-esbuild": "0.4.1",
    "@web/test-runner": "0.16.1",
    "@web/test-runner-commands": "0.7.0",
    "@web/test-runner-playwright": "0.10.0",
    "@webcomponents/template-shadowroot": "0.2.1",
    "@webcomponents-preview/cem-plugin-examples": "0.0.35",
    "@webcomponents-preview/cem-plugin-generate-readmes": "0.0.35",
    "@webcomponents-preview/cem-plugin-grouping": "0.0.35",
    "@webcomponents-preview/client": "0.0.41",
    "autoprefixer": "10.4.14",
    "axe-core": "4.7.1",
    "barrelsby": "2.7.0",
    "browser-sync": "2.29.2",
    "chai-a11y-axe": "1.4.1",
    "cross-env": "7.0.3",
    "esbuild": "0.17.19",
    "esbuild-plugin-clean": "1.0.1",
    "esbuild-sass-plugin": "2.9.0",
    "eslint": "8.40.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-tsdoc": "0.2.17",
    "fast-glob": "3.2.12",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.23",
    "postcss-preset-env": "8.3.2",
    "resolve": "1.22.2",
    "sass": "1.62.1",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typedoc": "0.24.7",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": "^18"
  },
  "customElements": "dist/custom-elements.json"
}
