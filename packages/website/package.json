{
  "name": "@kvlm/website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "clean": "shx rm -rf public/ui",
    "copy": "shx cp -r node_modules/@kvlm/ui/dist public/ui",
    "predev": "run-s clean copy",
    "dev": "tinacms dev -v --noTelemetry --command 'astro dev --host'",
    "prebuild": "run-s clean copy",
    "build": "astro build"
  },
  "dependencies": {
    "@kvlm/ui": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "tinacms": "1.5.28"
  },
  "devDependencies": {
    "@astrojs/mdx": "2.1.0",
    "@astrojs/netlify": "5.0.0",
    "@codemirror/language": "6.10.0",
    "@tinacms/cli": "1.5.39",
    "@types/node": "20.11.10",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/react-is": "18.2.4",
    "astro": "4.2.6",
    "graphql": "16.8.1",
    "graphql-tag": "2.12.6",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.33",
    "rollup": "4.9.6",
    "scheduler": "0.23.0",
    "shx": "0.3.4",
    "typescript": "5.3.3"
  }
}
