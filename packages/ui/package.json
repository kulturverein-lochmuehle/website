{
  "name": "@kvlm/ui",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "David Enke",
    "email": "david@enke.dev"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --ext .ts src",
    "clean": "shx rm -rf dist",
    "generate:barrels": "barrelsby --config .barrelsby.json",
    "generate:readmes": "wca analyze src --format markdown --outFiles '{dir}/README.md'",
    "generate": "run-p --print-name generate:*",
    "prebuild": "run-s --print-name clean generate",
    "build:components": "ts-node esbuild.config.ts",
    "build:types": "tsc --project tsconfig.types.json",
    "build": "run-p --print-name build:*",
    "predev": "run-s --print-name clean generate",
    "dev:build": "npm run build:components -- --watch --port 3500",
    "dev": "run-p --print-name dev:*"
  },
  "dependencies": {
    "lit": "2.6.1",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@types/browser-sync": "2.26.3",
    "@types/lodash-es": "4.17.6",
    "@types/node": "18.15.3",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "autoprefixer": "10.4.14",
    "barrelsby": "2.5.1",
    "browser-sync": "2.28.3",
    "esbuild": "0.17.11",
    "esbuild-copy-static-files": "0.1.0",
    "esbuild-plugin-clean": "1.0.1",
    "esbuild-sass-plugin": "2.6.0",
    "eslint": "8.36.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-tsdoc": "0.2.17",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "postcss-preset-env": "8.0.1",
    "resolve": "1.22.1",
    "sass": "1.59.2",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typedoc": "0.23.26",
    "typescript": "4.9.5",
    "web-component-analyzer": "2.0.0-next.4"
  },
  "engines": {
    "node": "^18"
  },
  "overrides": {
    "esbuild": "0.17.11"
  },
  "vaadin": {
    "disableUsageStatistics": true
  }
}
