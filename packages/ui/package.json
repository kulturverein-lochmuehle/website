{
  "name": "@kvlm/ui",
  "description": "UI components for kvlm.de",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "David Enke",
    "email": "david@kvlm.de"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/components.d.ts",
  "customElements": "src/custom-elements.json",
  "files": [
    "astro",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*.js": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./global.css": "./dist/index.css"
  },
  "scripts": {
    "lint": "eslint",
    "docs": "cross-env NODE_OPTIONS='--loader ts-node/esm --no-warnings' cem analyze --config cem.config.ts",
    "test:run": "cross-env NODE_OPTIONS='--loader ts-node/esm --no-warnings' wtr --config web-test-runner.config.ts",
    "test": "run-s -lns build:components test:run",
    "test:watch": "run-p -lns \"build:components --watch\" \"test:run --watch\"",
    "build:clean": "shx rm -rf dist",
    "build:index": "barrelsby --config .barrelsby.json",
    "build:docs": "npm run docs",
    "build:components": "cross-env NODE_OPTIONS='--loader ts-node/esm --no-warnings' node esbuild.config.ts",
    "build": "run-s -lns build:*",
    "dev:components": "npm run build:components -- --watch --port 3500",
    "dev:docs": "npm run docs -- --watch",
    "dev": "npm-run-all build:index build:docs --parallel --silent -ln dev:*"
  },
  "dependencies": {
    "lit": "3.3.0",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@csstools/css-parser-algorithms": "3.0.5",
    "@csstools/css-tokenizer": "3.0.4",
    "@custom-elements-manifest/analyzer": "0.10.4",
    "@davidenke/lint": "0.6.3",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@lit-labs/testing": "0.2.7",
    "@open-wc/semantic-dom-diff": "0.20.1",
    "@open-wc/testing": "4.0.0",
    "@open-wc/testing-helpers": "3.0.1",
    "@types/browser-sync": "2.29.0",
    "@types/esbuild-copy-static-files": "0.1.4",
    "@types/lodash-es": "4.17.12",
    "@types/mocha": "10.0.10",
    "@types/node": "22.16.3",
    "@web/dev-server-esbuild": "1.0.4",
    "@web/test-runner": "0.20.2",
    "@web/test-runner-commands": "0.9.0",
    "@web/test-runner-playwright": "0.11.1",
    "@webcomponents-preview/cem-plugin-examples": "0.0.43",
    "@webcomponents-preview/cem-plugin-generate-readmes": "0.0.43",
    "@webcomponents-preview/cem-plugin-grouping": "0.0.43",
    "@webcomponents-preview/client": "0.8.3",
    "@webcomponents/template-shadowroot": "0.2.1",
    "autoprefixer": "10.4.21",
    "axe-core": "4.10.3",
    "barrelsby": "2.8.1",
    "chai-a11y-axe": "1.5.0",
    "cross-env": "7.0.3",
    "esbuild": "0.25.6",
    "esbuild-copy-static-files": "0.1.0",
    "esbuild-plugin-d.ts": "1.3.1",
    "esbuild-sass-plugin": "3.3.1",
    "eslint": "9.30.1",
    "fast-glob": "3.3.3",
    "jiti": "2.4.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.5.6",
    "postcss-preset-env": "10.2.4",
    "prettier": "3.6.2",
    "resolve": "1.22.10",
    "shx": "0.4.0",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.16",
    "tslib": "2.8.1",
    "typedoc": "0.28.7",
    "typescript": "5.8.3"
  }
}
