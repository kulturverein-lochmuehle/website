{
  "name": "@kvlm/ui",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "David Enke",
    "email": "david@enke.dev"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./global.css": "./dist/index.css",
    "./*": "./dist/*.js",
    "./*.js": "./dist/*.js"
  },
  "scripts": {
    "lint": "eslint --ext .ts src",
    "clean": "shx rm -rf dist",
    "generate": "barrelsby --config .barrelsby.json",
    "docs": "cross-env NODE_OPTIONS='--loader ts-node/esm' NODE_NO_WARNINGS=1 cem analyze --config custom-elements-manifest.config.ts",
    "test:run": "cross-env NODE_OPTIONS='--loader ts-node/esm' NODE_NO_WARNINGS=1 wtr --config web-test-runner.config.ts",
    "test": "run-s --print-name build:components test:run",
    "test:watch": "run-p --print-name \"build:components --watch\" \"test:run --watch\"",
    "prebuild": "run-s --print-name clean generate",
    "build:components": "ts-node esbuild.config.ts",
    "build:types": "tsc --project tsconfig.types.json",
    "build": "run-p --print-name build:*",
    "predev": "run-s --print-name clean generate",
    "dev:docs": "npm run docs -- --watch",
    "dev:components": "npm run build:components -- --watch --port 3500",
    "dev": "run-p --print-name dev:*"
  },
  "dependencies": {
    "lit": "2.7.0",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@csstools/css-parser-algorithms": "2.1.0",
    "@csstools/css-tokenizer": "2.1.0",
    "@custom-elements-manifest/analyzer": "0.8.0",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@lit-labs/testing": "0.2.0",
    "@open-wc/semantic-dom-diff": "0.19.9",
    "@open-wc/testing": "3.1.7",
    "@open-wc/testing-helpers": "2.2.0",
    "@types/browser-sync": "2.27.0",
    "@types/lodash-es": "4.17.7",
    "@types/mocha": "10.0.1",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "@web/dev-server-esbuild": "0.3.5",
    "@web/test-runner": "0.15.2",
    "@web/test-runner-commands": "0.6.5",
    "@web/test-runner-playwright": "0.9.0",
    "@webcomponents/template-shadowroot": "0.2.1",
    "@webcomponents-preview/cem-plugin-examples": "0.0.35",
    "@webcomponents-preview/cem-plugin-generate-readmes": "0.0.35",
    "@webcomponents-preview/cem-plugin-grouping": "0.0.35",
    "@webcomponents-preview/client": "0.0.34",
    "autoprefixer": "10.4.14",
    "axe-core": "4.6.3",
    "barrelsby": "2.5.1",
    "browser-sync": "2.29.1",
    "chai-a11y-axe": "1.4.1",
    "cross-env": "7.0.3",
    "esbuild": "0.17.15",
    "esbuild-plugin-clean": "1.0.1",
    "esbuild-sass-plugin": "2.8.0",
    "eslint": "8.37.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-tsdoc": "0.2.17",
    "fast-glob": "3.2.12",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "postcss-preset-env": "8.3.0",
    "resolve": "1.22.1",
    "sass": "1.60.0",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typedoc": "0.23.28",
    "typescript": "5.0.3"
  },
  "engines": {
    "node": "^18"
  },
  "overrides": {
    "esbuild": "0.17.15"
  },
  "customElements": "dist/custom-elements.json"
}
