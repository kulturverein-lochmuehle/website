---
import { getEntryBySlug } from 'astro:content';

import { isInlineSection } from '@utils/astro.utils';

import Page from '@components/page.component.astro';
import Sections from '@components/sections.component.astro';

const { slug: currentSlug } = Astro.params;
const current = currentSlug && await getEntryBySlug('verein', currentSlug);
---

{
  current && !isInlineSection(current.id) ? (
    <Page />
  ) : (
    <Sections />
  )
}
