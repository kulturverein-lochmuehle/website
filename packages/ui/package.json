{
  "name": "@kvlm/ui",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "David Enke",
    "email": "david@enke.dev"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "astro",
    "dist"
  ],
  "exports": {
    "./global.css": "./dist/index.css",
    "./*.js": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./astro/*.js": {
      "import": "./astro/*.js",
      "types": "./astro/*.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint --ext .ts src",
    "docs": "cross-env NODE_OPTIONS='--loader ts-node/esm' NODE_NO_WARNINGS=1 cem analyze --config cem.config.ts",
    "test:run": "cross-env NODE_OPTIONS='--loader ts-node/esm' NODE_NO_WARNINGS=1 wtr --config web-test-runner.config.ts",
    "test": "run-s -lns build:components test:run",
    "test:watch": "run-p -lns \"build:components --watch\" \"test:run --watch\"",
    "build:clean": "shx rm -rf astro dist",
    "build:index": "barrelsby --config .barrelsby.json",
    "build:components": "cross-env NODE_OPTIONS='--loader ts-node/esm' NODE_NO_WARNINGS=1 node esbuild.config.ts",
    "build:types": "tsc --project tsconfig.types.json",
    "build:aliases": "tsc-alias -p tsconfig.types.json",
    "build:docs": "npm run docs",
    "build": "run-s -lns build:*",
    "predev": "npm run build:index",
    "dev:components": "npm run build:components -- --watch --port 3500",
    "dev:docs": "npm run docs -- --watch",
    "dev": "run-p -lns dev:*"
  },
  "dependencies": {
    "lit": "2.8.0",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@csstools/css-parser-algorithms": "2.3.1",
    "@csstools/css-tokenizer": "2.2.0",
    "@custom-elements-manifest/analyzer": "0.8.4",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@lit-labs/testing": "0.2.1",
    "@open-wc/semantic-dom-diff": "0.20.0",
    "@open-wc/testing": "3.2.0",
    "@open-wc/testing-helpers": "2.3.0",
    "@types/browser-sync": "2.27.0",
    "@types/lodash-es": "4.17.8",
    "@types/mocha": "10.0.1",
    "@types/node": "18.16.20",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "@web/dev-server-esbuild": "0.4.1",
    "@web/test-runner": "0.17.0",
    "@web/test-runner-commands": "0.8.0",
    "@web/test-runner-playwright": "0.10.1",
    "@webcomponents/template-shadowroot": "0.2.1",
    "@webcomponents-preview/cem-plugin-examples": "0.0.35",
    "@webcomponents-preview/cem-plugin-generate-readmes": "0.0.35",
    "@webcomponents-preview/cem-plugin-grouping": "0.0.35",
    "@webcomponents-preview/client": "0.5.6",
    "autoprefixer": "10.4.15",
    "axe-core": "4.7.2",
    "barrelsby": "2.8.0",
    "browser-sync": "2.29.3",
    "chai-a11y-axe": "1.5.0",
    "cross-env": "7.0.3",
    "esbuild": "0.19.2",
    "esbuild-plugin-clean": "1.0.1",
    "esbuild-sass-plugin": "2.12.0",
    "eslint": "8.47.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-tsdoc": "0.2.17",
    "fast-glob": "3.3.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.28",
    "postcss-preset-env": "9.1.1",
    "resolve": "1.22.4",
    "sass": "1.66.1",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.7",
    "tslib": "2.6.2",
    "typedoc": "0.24.8",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": "^18||^20"
  },
  "customElements": "dist/custom-elements.json"
}
