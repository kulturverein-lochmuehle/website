{
  "name": "@kvlm/ui",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "David Enke",
    "email": "david@enke.dev"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "customElements": "dist/custom-elements.json",
  "files": [
    "astro",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*.js": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./astro/*.js": {
      "import": "./astro/*.js",
      "types": "./astro/*.d.ts"
    },
    "./global.css": "./dist/index.css"
  },
  "scripts": {
    "lint": "eslint --ext .ts src",
    "docs": "cross-env NODE_OPTIONS='--loader ts-node/esm' NODE_NO_WARNINGS=1 cem analyze --config cem.config.ts",
    "test:run": "cross-env NODE_OPTIONS='--loader ts-node/esm' NODE_NO_WARNINGS=1 wtr --config web-test-runner.config.ts",
    "test": "run-s -lns build:components test:run",
    "test:watch": "run-p -lns \"build:components --watch\" \"test:run --watch\"",
    "build:clean": "shx rm -rf astro dist",
    "build:index": "barrelsby --config .barrelsby.json",
    "build:components": "cross-env NODE_OPTIONS='--loader ts-node/esm' NODE_NO_WARNINGS=1 node esbuild.config.ts",
    "build:types": "tsc --project tsconfig.types.json",
    "build:aliases": "tsc-alias -p tsconfig.types.json",
    "build:docs": "npm run docs",
    "build": "run-s -lns build:*",
    "predev": "npm run build:index",
    "dev:components": "npm run build:components -- --watch --port 3500",
    "dev:docs": "npm run docs -- --watch",
    "dev": "run-p -lns dev:*"
  },
  "dependencies": {
    "lit": "3.1.0",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@csstools/css-parser-algorithms": "2.3.2",
    "@csstools/css-tokenizer": "2.2.1",
    "@custom-elements-manifest/analyzer": "0.9.0",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@lit-labs/testing": "0.2.3",
    "@open-wc/semantic-dom-diff": "0.20.1",
    "@open-wc/testing": "4.0.0",
    "@open-wc/testing-helpers": "3.0.0",
    "@types/browser-sync": "2.27.5",
    "@types/lodash-es": "4.17.12",
    "@types/mocha": "10.0.6",
    "@types/node": "20.10.2",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@web/dev-server-esbuild": "1.0.1",
    "@web/test-runner": "0.18.0",
    "@web/test-runner-commands": "0.9.0",
    "@web/test-runner-playwright": "0.11.0",
    "@webcomponents/template-shadowroot": "0.2.1",
    "@webcomponents-preview/cem-plugin-examples": "0.0.39",
    "@webcomponents-preview/cem-plugin-generate-readmes": "0.0.39",
    "@webcomponents-preview/cem-plugin-grouping": "0.0.39",
    "@webcomponents-preview/client": "0.5.6",
    "autoprefixer": "10.4.16",
    "axe-core": "4.8.2",
    "barrelsby": "2.8.1",
    "chai-a11y-axe": "1.5.0",
    "cross-env": "7.0.3",
    "esbuild": "0.19.8",
    "esbuild-plugin-clean": "1.0.1",
    "esbuild-sass-plugin": "2.16.0",
    "eslint": "8.55.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-lit": "1.10.1",
    "eslint-plugin-tsdoc": "0.2.17",
    "fast-glob": "3.3.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.32",
    "postcss-preset-env": "9.3.0",
    "resolve": "1.22.8",
    "sass": "1.69.5",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.8",
    "tslib": "2.6.2",
    "typedoc": "0.25.4",
    "typescript": "5.3.2"
  }
}
