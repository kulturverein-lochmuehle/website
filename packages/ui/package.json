{
  "name": "@kvlm/ui",
  "description": "UI components for kvlm.de",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "David Enke",
    "email": "david@kvlm.de"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/components.d.ts",
  "customElements": "public/custom-elements.json",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*.js": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./global.css": "./dist/index.css"
  },
  "scripts": {
    "lint": "eslint",
    "docs": "cross-env NODE_OPTIONS='--loader ts-node/esm' NODE_NO_WARNINGS=1 cem analyze --config cem.config.ts",
    "test": "cross-env NODE_OPTIONS='--loader ts-node/esm' NODE_NO_WARNINGS=1 wtr --config web-test-runner.config.ts",
    "test:watch": "npm test -- --watch",
    "bundle": "vite build",
    "build": "run-s -ns docs bundle",
    "dev:docs": "npm run docs -- --watch",
    "dev:components": "vite dev --port 3500",
    "dev": "npm-run-all docs --parallel --silent -ln dev:*"
  },
  "dependencies": {
    "@enke.dev/lit-utils": "0.0.3"
  },
  "peerDependencies": {
    "lit": "^3",
    "lodash-es": "^4.17",
    "urlpattern-polyfill": "^10"
  },
  "devDependencies": {
    "@csstools/css-parser-algorithms": "3.0.5",
    "@csstools/css-tokenizer": "3.0.4",
    "@custom-elements-manifest/analyzer": "0.10.4",
    "@enke.dev/lint": "0.6.4",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@lit-labs/testing": "0.2.7",
    "@open-wc/semantic-dom-diff": "0.20.1",
    "@open-wc/testing": "4.0.0",
    "@open-wc/testing-helpers": "3.0.1",
    "@remcovaes/web-test-runner-vite-plugin": "1.2.2",
    "@types/browser-sync": "2.29.0",
    "@types/esbuild-copy-static-files": "0.1.4",
    "@types/lodash-es": "4.17.12",
    "@types/mocha": "10.0.10",
    "@types/node": "22.16.3",
    "@web/test-runner": "0.20.2",
    "@web/test-runner-commands": "0.9.0",
    "@web/test-runner-playwright": "0.11.1",
    "@webcomponents-preview/cem-plugin-examples": "0.0.44",
    "@webcomponents-preview/cem-plugin-generate-readmes": "0.0.44",
    "@webcomponents-preview/cem-plugin-grouping": "0.0.44",
    "@webcomponents-preview/client": "0.8.7",
    "@webcomponents/template-shadowroot": "0.2.1",
    "autoprefixer": "10.4.21",
    "axe-core": "4.10.3",
    "barrelsby": "2.8.1",
    "chai-a11y-axe": "1.5.0",
    "cross-env": "7.0.3",
    "esbuild": "0.25.6",
    "eslint": "9.31.0",
    "eslint-formatter-junit": "8.40.0",
    "fast-glob": "3.3.3",
    "jiti": "2.4.2",
    "lit": "3.3.1",
    "lodash-es": "4.17.21",
    "npm-run-all": "4.1.5",
    "postcss": "8.5.6",
    "postcss-preset-env": "10.2.4",
    "prettier": "3.6.2",
    "resolve": "1.22.10",
    "sass": "1.89.2",
    "shx": "0.4.0",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.16",
    "tslib": "2.8.1",
    "typedoc": "0.28.7",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-plugin-banner": "0.8.1",
    "vite-plugin-checker": "0.10.0",
    "vite-plugin-mkcert": "1.17.8",
    "vite-plugin-node-polyfills": "0.24.0",
    "vite-plugin-static-copy": "3.1.1",
    "vite-plugin-top-level-await": "1.5.0"
  }
}
