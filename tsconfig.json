{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "declaration": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "jsxFactory": "h",
    "moduleResolution": "nodenext",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "esnext"
  }
}
